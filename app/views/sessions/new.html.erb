<%= form_tag '/login' do %>
 <div >
    <form >
      <div>
        <%= text_field_tag :username %>
        <label for="last_name">Username</label>
      </div>
      <div>
        <%= password_field_tag :password %>
        <label for="password">Password</label>
      </div>
      <div>
        <%= submit_tag "Login" %>
      </div>
    </form>
  </div>
<% end %>